tgg:version rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://eatld.et.tu-dresden.de/tgg#DataSet
tgg:DataSet rdf:type owl:Class ;
            rdfs:comment "A data source contains information how to access a triplestore to gather further information about this data set" ;
            rdfs:label "data set" .


###  http://eatld.et.tu-dresden.de/tgg#Domain
tgg:Domain rdf:type owl:Class ;
           rdfs:comment "One or multiple domains can be used in TGG for the source or the target side of a transformation. A domain has a name and can have multiple data set containing ontologies used in this domain." ;
           rdfs:label "domain" .


###  http://eatld.et.tu-dresden.de/tgg#Rule
tgg:Rule rdf:type owl:Class ;
         rdfs:comment "A rule defines one TGG rule following the structure of the TGG SPARQL transformation engine." ;
         rdfs:label "rule" .


###  http://eatld.et.tu-dresden.de/tgg#RuleSet
tgg:RuleSet rdf:type owl:Class ;
            rdfs:comment "A rule set contains all rules for the bidirectional transformation between two domains. These domains are referenced by 'tgg:hasSourceDomain' and 'tgg:hasTargetDomain'." ;
            rdfs:label "rule set" .


###  http://eatld.et.tu-dresden.de/tgg#RuleStore
tgg:RuleStore rdf:type owl:Class ;
              rdfs:comment "The rule store is the entry point for the TGG Rule Editor and the TGG SPARQL engine. It can hold multiple rulesets and domains." ;
              rdfs:label "rule store" .


